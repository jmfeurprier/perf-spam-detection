{
	"name"        : "perf/spam-detection",
	"type"        : "library",
	"license"     : "MIT",
	"description" : "Allows spam detection from suspicious form submissions.",
	"require"     : {
		"php" : ">=8.2"
	},
	"require-dev": {
		"ext-xdebug"                : "*",
		"phing/phing"               : "^2.16",
		"phpunit/phpunit"           : "^10.4",
		"phpmd/phpmd"               : "^2.8",
		"squizlabs/php_codesniffer" : "^3.5",
		"phpstan/phpstan": "^1.10",
		"rector/rector": "^0.18.6"
	},
	"autoload" : {
		"psr-4" : {
			"perf\\SpamDetection\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"perf\\SpamDetection\\" : "tests/unit/src/"
		}
	}
}
